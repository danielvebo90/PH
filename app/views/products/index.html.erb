
<div class="container">

  <div class="row">

    <div class="col-sm-12">


        <div class="text-center">
                <h1>PRODUCTOS</h1>
        </div>


      <div class="text-right">
        <%= link_to "PUBLICAR PRODUCTO", new_product_path,class: "btn btn-primary btn-sm " %>

      </div>

      <table class="table text center">
        <tr>
          <th>Id</th>
          <th>Nombre</th>
          <th>URL</th>
          <th>DESCRIPTION</th>
        </tr>
        <% @vproducts.each do |p| %>
          <tr>
            <td><%= p.id %></td>
            <td><%= p.name %></td>
            <td><%= p.url %></td>
            <td><%= p.description %></td>
          </tr>
        <% end %>

      </table>

    </div>

  </div>

</div>
